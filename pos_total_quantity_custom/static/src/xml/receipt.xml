<?xml version="1.0" encoding="UTF-8"?>
<templates id="coupon_template" xml:space="preserve">

    <t t-inherit="point_of_sale.OrderReceipt" t-inherit-mode="extension">
    <xpath expr="//div[hasclass('pos-receipt-amount')]" position="after">
        <br/>

           <strong>
               Quantity Total
               <span t-esc="receipt.quantity_count"/>
           </strong>
        <br/>
         <strong>
                Total Items
               <span t-esc="receipt.total_items"/>
           </strong>
    </xpath>
  </t>



</templates>