-the orange is name in english and name in arabic take from Odoo translated filed not new field  
-the black circle for note ( also we want to use note from Odoo not new filed ) 
-The invoice number take the last 3 number from order number as you can see in blue circle 

==============================================
<div class="pos-sale-ticket">
	<!--<div style="font-size: 70%; text-align:center;"><div><span t-esc='receipt.date.localestring'/><span t-esc='receipt.name'/></div></div><br/>-->
	<t t-if='receipt.company.logo'>
		<img style="width: 150px; display: table; margin: auto;" t-att-src='receipt.company.logo' alt="Logo"/>
	</t>
	<div style="text-align:center; font-size: 90%; padding-top: 0px;padding-bottom: 1px;">
             فرع: الحمراء
            </div>
	<!--br/>-->
	<div style="text-align:center; font-size: 90%; padding-top: 0px;padding-bottom: 1px;">
             Branch: Al Hamra
             </div>
<!--<br/>-->

	<!-- Orderlines -->
	<div class='orderlines'>
		<div style="text-align:center; font-size: auto; border-top: 1px dashed black; border-bottom: 1px dashed black;padding-top: 3px;padding-bottom: 3px;">
			<div>Receipt(إيصال) : 
				<span t-esc='receipt.name' />
			</div>
			<!--<br/>-->
			<div>Date (التاريخ) : 
				<spam t-esc='receipt.date.localestring' />
			</div>
			<!--<br/>-->
			<t t-if='receipt.client'>
				<div>Client( اسم العميل) : 
					<t t-esc='receipt.client' />
				</div>
				<!--<br/>-->
			</t>
			<t t-if='receipt.cashier'>
				<div class='cashier'>
					<div>Served by (قدم بواسطة) : 
						<t t-esc='receipt.cashier' />
					</div>
				</div>
			</t>
			<t t-if="receipt.token_no">
				<h3 style="font-size: 20px;margin-top: 5px;margin-bottom: 5px;"> Invoice 
					<t t-esc='receipt.token_no' /> (فاتورة) 
				</h3>
			</t>
		</div>
		<!--<br/>
		<br/>-->
		<table style="width: 100%;">
			<tr style="border-bottom: 1px solid black;font-size:13px;">
				<th style="width: 60%; text-align:left;">Product
					<br/> اسم المنتج
				</th>
				<th style="text-align: center;">Qty
					<br/> الكمية
				</th>
				<th style="text-align: center;">Unit Price
					<br/>سعر الوحدة
				</th>
				<th style="text-align: center;">Amount
					<br/>مبلغ
				</th>
			</tr>
			<tr t-foreach="receipt.orderlines" t-as="line" style="font-size: 13px;font-family: initial;">
				<td>
					<div style="padding-top: auto;padding-bottom: auto;">
						<span t-esc='line.product_name_wrapped[0]'/>
						<t t-if="line.product_translation_name">
							<!--<br/> ....-->
							<span t-esc="line.product_translation_name"/>
						</t>
						<t t-if='line.discount !== 0'>
							<h5 style="margin-top: 0%;margin-bottom: 0%;font-size: 12px;color: #848484;">
								<t t-esc='line.discount' />% Discount(خصم) 
                                
							</h5>
						</t>
					</div>
				</td>
				<td style="text-align: center;">
					<span t-esc="line.quantity"/>
				</td>
				<td style="text-align: center;">
					<span t-esc="widget.format_currency_no_symbol(line.price)"/>
				</td>
				<td style="text-align: center;">
					<span t-esc='widget.format_currency_no_symbol(line.price_display)'/>
				</td>
				<t t-if="widget.pos.config.on_product_line">
					<t t-if="line.order_line_note">
						<tr>
							<td colspan="3">
								<t t-esc="line.order_line_note" />
							</td>
						</tr>
					</t>
				</t>
			</tr>
		</table>
	</div>
	<div>
		<!--<br/>-->
		<t t-if="widget.pos.config.on_order and widget.pos.config.receipt_order_note">
			<t t-if="order.get_order_note()!=''">
				<table>
					<tr>
						<td style="width: auto;">Note:</td>
						<td style="text-align: left;">
							<t t-esc="order.get_order_note()"/>
						</td>
					</tr>
				</table>
			</t>
		</t>
		<!-- Subtotal -->
		<t t-set='taxincluded' t-value='Math.abs(receipt.subtotal - receipt.total_with_tax) &lt;= 0.000001' />
		<t t-if='!taxincluded'>
			<!--<br/>-->
			<div style="font-weight: 600;text-align: right; font-size: 17px;border-top: 2px solid;margin-left: 25%; padding-top: 2%;">Subtotal(المجموع الفرعي) : 
				<span t-esc='widget.format_currency(receipt.subtotal)' class="pos-receipt-right-align"/>
			</div>
			<t t-foreach='receipt.tax_details' t-as='tax'>
				<div style="font-weight: 700;text-align: right;">
					<t t-esc='tax.name' />
					<span t-esc='widget.format_currency_no_symbol(tax.amount)' class="pos-receipt-right-align"/>
				</div>
			</t>
		</t>
		<!-- Total -->
		<!--<br/>-->
		<div style="font-size: 13px;text-align: right;font-weight: 600; border-top: 2px solid;margin-left: 20%;padding-top: 2%;">
                TOTAL(مجموع) :
                
			<span t-esc='widget.format_currency(receipt.total_with_tax)'/>
		</div>
		<!--<br/>-->
		<!-- Extra Payment Info -->
		<t t-if='receipt.total_discount'>
			<div style="font-size: 14px;text-align: right;border-top: 1px solid;margin-left: 30%;padding-top: 2%;">
                    Discounts(الخصومات)
                    
				<span t-esc='widget.format_currency(receipt.total_discount)'/>
			</div>
		</t>
		<!--<br/>-->
		<t t-if='taxincluded'>
			<t t-foreach='receipt.tax_details' t-as='tax'>
				<div style="font-size: 15px; text-align: right; font-weight: 700; border-top: 1px solid;margin-left: 30%;padding-top: 2%;">
					<t t-esc='tax.name' />
					<span t-esc='widget.format_currency_no_symbol(tax.amount)'/>
				</div>
			</t>
			<div style="font-size: 12px; text-align: right; font-weight: 700;">
                    Total Taxes(مبلغ الضريبة) :
                    
				<span t-esc='widget.format_currency(receipt.total_tax)'/>
			</div>
		</t>
	</div>
	<!--<br/>
	<br/>-->
	<div style="border-top: 1px dashed black;padding-top: 5%;border-bottom: 1px dashed black;">
		<!-- Payment Lines -->
		<t t-foreach='paymentlines' t-as='line'>
			<div style="font-size: 14px;">
				<t t-esc='line.name' />
				<span t-esc='widget.format_currency_no_symbol(line.get_amount())' class="pos-receipt-right-align"/>
			</div>
		</t>
		<!--<br/>-->
		<div class="receipt-change" style="font-size: 14px;">
                    CHANGE(الباقي)
                    
			<span t-esc='widget.format_currency(receipt.change)' class="pos-receipt-right-align"/>
		</div>
		<!--<br/>-->
	</div>
	<div style="font-size: 80%; text-align:center;">
		<!--<br/>-->
		<t t-if='!receipt.company.logo'>
			<h2 class="pos-receipt-center-align">
				<t t-esc='receipt.company.name' />
			</h2>
		</t>
		<t t-if='receipt.company.contact_address'>
			<div>
				<t t-esc='receipt.company.contact_address' />
			</div>
		</t>
		<t t-if='receipt.company.phone'>
			<div>Tel:
				<t t-esc='receipt.company.phone' />
			</div>
		</t>
		<t t-if='receipt.company.website'>
			<div>
				<t t-esc='receipt.company.website' />
			</div>
		</t>
		<t t-if='receipt.header_html'>
			<t t-raw='receipt.header_html' /></t>
		<t t-if='!receipt.header_html and receipt.header'>
			<div>
				<t t-esc='receipt.header' />
			</div>
		</t>
		<!--<br/>-->
	</div>
	<div style="font-size: 40%; text-align:center;">
		<t t-if="widget.pos.company.name_arabic">
			<div style="font-size:18pt;font-weight:bold">
				<span t-esc="widget.pos.company.name_arabic" style="text-size:12pt"/>
				<!--<br/>-->
			</div>
		</t>
		<t t-if="widget.pos.company.street_arabic">
			<span t-esc="widget.pos.company.street_arabic"/>،
            
		</t>
		<t t-if="widget.pos.company.street2_arabic">
			<span t-esc="widget.pos.company.street2_arabic"/>،
            
		</t>
		<t t-if="widget.pos.company.city_arabic">
			<span t-esc="widget.pos.company.city_arabic"/>،
            
		</t>
		<t t-if="widget.pos.company.state_arabic">
			<span t-esc="widget.pos.company.state_arabic"/>.
            
		</t>
	</div>
	<div class='before-footer' />
	<!-- Footer -->
	<div t-if='receipt.footer_html'  class="pos-receipt-center-align" style="font-size: 12px;">
		<t t-raw='receipt.footer_html'/>
	</div>
	<div t-if='!receipt.footer_html and receipt.footer'  class="pos-receipt-center-align" style="font-size: 13px;">
		<!--<br/>-->
		<t t-esc='receipt.footer'/>
		<!--<br/>
		<br/>-->
	</div>
	<div class='after-footer' style="font-size: 14px;">
		<t t-foreach='paymentlines' t-as='line'>
			<t t-if='line.ticket'>
				<!--<br/>-->
				<div class="pos-payment-terminal-receipt">
					<t t-raw='line.ticket'/>
				</div>
			</t>
		</t>
	</div>
<br/>
	<div style="text-align:center;">
             Tax ID:  الرقم الضريبي: 302090125600003 
            </div>
</div>